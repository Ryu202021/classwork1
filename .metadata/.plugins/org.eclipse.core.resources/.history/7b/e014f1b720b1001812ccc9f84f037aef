package com.bayviewglen.dayone;

public class ExampleOne {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Comments are meant for developers/programmers to describe what code does
		
		/*
		 * This comment spans multiple lines
		 */
		
		System.out.println("This is going to t'he console1.");
		System.out.print("This is \"going\" to the console2.");
		System.out.println("Thi\\s is going to the console3.");
		System.out.print("This is going to the console4.");
	
		// escape sequence escapes the regular purpose of a character
		// \\ slash, \n enter, \" quote, \' single quoute, \t is tab
	}

}
